PrimaryWD=$PWD

cd $PrimaryWD/MasterFile/SourceFiles

make -f makefile_Master_Lin_API_32Bit clean
make -f makefile_Master_Lin_API_32Bit

cd $PrimaryWD

mkdir TestFolder_32Bit

cp -f -p $PrimaryWD/MasterFile/SourceFiles/VP8_Tester_API_32Bit.exe $PrimaryWD/TestFolder_32Bit/VP8_Tester_API_32Bit.exe
cd $PrimaryWD/SupportingPlugInFiles/VP8DebugPlugIn/MasterFile/SourceFiles

make -f makefile_Debug_Lin_API_32Bit clean
make -f makefile_Debug_Lin_API_32Bit

cd $PrimaryWD

cp -f -p $PrimaryWD/SupportingPlugInFiles/VP8DebugPlugIn/MasterFile/SourceFiles/VP8vNewest_PlugIn_DLib_DMode_32Bit.exe $PrimaryWD/TestFolder_32Bit/VP8vNewest_PlugIn_DLib_DMode_32Bit.exe
cd $PrimaryWD/SupportingPlugInFiles/VP8ReleasePlugIn/MasterFile/SourceFiles

make -f makefile_Release_Lin_New_API_32Bit clean
make -f makefile_Release_Lin_New_API_32Bit
make -f makefile_Release_Lin_Old_API_32Bit clean
make -f makefile_Release_Lin_Old_API_32Bit

cd $PrimaryWD

cp -f -p $PrimaryWD/SupportingPlugInFiles/VP8ReleasePlugIn/MasterFile/SourceFiles/VP8vNewest_PlugIn_RLib_RMode_32Bit.exe $PrimaryWD/TestFolder_32Bit/VP8vNewest_PlugIn_RLib_RMode_32Bit.exe
cp -f -p $PrimaryWD/SupportingPlugInFiles/VP8ReleasePlugIn/MasterFile/SourceFiles/VP8vOldest_PlugIn_RLib_RMode_32Bit.exe $PrimaryWD/TestFolder_32Bit/VP8vOldest_PlugIn_RLib_RMode_32Bit.exe
